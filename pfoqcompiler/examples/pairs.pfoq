decl pairs(a){
    if(|a|>1)then{
        qcase(a[0]) of {
            0->qcase(a[1]) of{
                0->call pairs((a-[0,1]));,
                1->skip;
            },
            1->qcase(a[1]) of{
                0->skip;,
                1->call pairs(a-[0,1]);
            }
        }
    }else{a[0]*=NOT;}
}
::
define q;
::
call pairs(q);
