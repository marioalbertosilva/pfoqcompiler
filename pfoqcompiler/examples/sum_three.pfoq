decl zero(a){ qcase (a[0]) of {0 -> call zero(a-[0]);, 1 -> call one(a-[0]); } }
decl one(a){ qcase (a[0]) of {0 -> call one(a-[0]);, 1 -> call two(a-[0]); } }
decl two(a){ qcase (a[0]) of {0 -> call two(a-[0]);, 1 -> call three(a-[0]); } }
decl three(a){
    if(|a|>1)then{qcase (a[0]) of {0 -> call three(a-[0]);, 1 -> skip; }}else{call oplus(a);}}
decl oplus(a){ a[-1] *= NOT; }
::
define q;
::
call zero(q);
